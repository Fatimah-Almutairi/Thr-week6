generator client {
  provider = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}


model movies {
  id String  @id @default(uuid())
  name String @unique
  genre  Role  @default(Drama)
  rating Int
  duration Int
  createdat DateTime
}

enum Role {
  Drama 
  Action
  Comady
}